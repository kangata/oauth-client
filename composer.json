{
    "name": "quetzal-studio/oauth-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Ata",
            "email": "quetzalskeith@gmail.com"
        }
    ],
    "require": {
        "laravel/passport": "^11.8",
        "spatie/laravel-permission": "^5.10"
    },
    "require-dev": {
        "laravel/pint": "^1.10"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "QuetzalStudio\\OAuthClient\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "QuetzalStudio\\OAuthClient\\OAuthClientServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
